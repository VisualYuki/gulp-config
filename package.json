	"scripts": {
		"dev:serve": "cross-env NODE_ENV=development npm-run-all --parallel gulp-serve webpack-watch",
		"dev:build": " cross-env NODE_ENV=development gulp build && cross-env NODE_ENV=development webpack",
		"prod:serve": "cross-env NODE_ENV=production npm-run-all --parallel gulp-serve webpack-watch",
		"prod:build": "cross-env NODE_ENV=production gulp build && cross-env NODE_ENV=production webpack ",
		"lint": "npm run lint:scss & npm run lint:pug",
		"lint:scss": "stylelint ./src/scss",
		"lint:pug": "pug-lint ./src/pug",
		"gulp-serve": "gulp dev",
		"webpack-watch": "webpack watch",
		"start": "npm update --save && npm install && npm run dev:serve",
		"scripts": "cross-env NODE_ENV=development webpack",
		"pug2html": "gulp pug2html",
		"include_pug2html": "gulp include_pug2html",
		"styles": "gulp styles",
		"minJpg": "gulp minJpg",
		"minPng": "gulp minPng",
		"convWebp": "gulp convWebp",
		"minSvg": "gulp minSvg",
		"allImg": "gulp allImg",
		"copyToDist": "gulp copyToDist",
		"clearDist": "gulp clearDist",
		"getLibs": "gulp getLibs",
		"prettier": "prettier --write src public public"
	},
