mixin setLazyPicture(className,folder,name,ext,alt)
	picture(class=`${className}`)
		source.lazyload(srcset="../gifs/preloader.gif" data-srcset=`..//webp-img/${folder}/${name}.webp` type="image/webp")
		img.lazyload(src="../gifs/preloader.gif" data-src=`..//min-img/${folder}/${name}.${ext}` alt=`${alt}`)
